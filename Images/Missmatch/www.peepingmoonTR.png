<script>
    const accessToken = 'IGQWRObkZAXQk9xR2RyQUhpRnBQb1JFTDFDek53QnZApNXA1c1B6U3ZAncVpic2JWOEN3TWk0OXFEb0JwTXB0cGZAWOW5fSHNnMjNZAajJ5UXYzRjJOeG5WRldkdjhTX3ZApeFZAGMWpuMFFadnJpUmhxMXJncDY1TnVWTkUZD';

    async function fetchInstagramPosts() {
        try {
            const response = await fetch(`https://graph.instagram.com/me/media?fields=id,media_type,media_url,thumbnail_url,permalink&access_token=${accessToken}&limit=12`);
            const data = await response.json();
            console.log("🚀 ~ fetchInstagramPosts ~ data:", data.data)
            return data.data;
        } catch (error) {
            console.error('Error fetching Instagram posts:', error);
            return [];
        }
    }

    async function exchangeForLongLivedToken(shortToken) {
        try {
            const response = await fetch(`https://graph.instagram.com/refresh_access_token?grant_type=ig_refresh_token&client_secret=472bd809719c99a297638c73997810fb&access_token=${shortToken}`);
            const data = await response.json();
            console.log("🚀 ~ exchangeForLongLivedToken ~ data:", data)
            return data.access_token;
        } catch (error) {
            console.error('Error exchanging for long-lived token:', error);
            return null;
        }
    }

    async function displayInstagramPosts() {
        const instafeed = document.getElementById('instafeed');
        const posts = await fetchInstagramPosts();
        if (posts == undefined || posts.length == 0) {
            $("#insta_feed").hide()
        }
        posts?.forEach(post => {
            const postElement = document.createElement('div');
            postElement.className = 'col-lg-3 col-md-3 col-sm-3 col-4 mb-3 insta_images h-100';
            postElement.innerHTML = `
         
          <a href="${post.permalink}" target="_blank">
            <img class="lazy" src="${post.thumbnail_url ?? post.media_url}" alt="Instagram Post" width="100%">
           
          </a>
          <a href="${post.permalink}" target="_blank" class="img_show">
          <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="100" height="100" viewBox="0 0 16 16" style="text-align: center;
              width: 16%;">
          <path d="M 5 1 C 2.791 1 1 2.791 1 5 L 1 11 C 1 13.209 2.791 15 5 15 L 11 15 C 13.209 15 15 13.209 15 11 L 15 5 C 15 2.791 13.209 1 11 1 L 5 1 z M 5 2 L 11 2 C 12.657 2 14 3.343 14 5 L 14 11 C 14 12.657 12.657 14 11 14 L 5 14 C 3.343 14 2 12.657 2 11 L 2 5 C 2 3.343 3.343 2 5 2 z M 5 3 C 3.895 3 3 3.895 3 5 L 3 11 C 3 12.105 3.895 13 5 13 L 11 13 C 12.105 13 13 12.105 13 11 L 13 5 C 13 3.895 12.105 3 11 3 L 5 3 z M 11.5 4 C 11.776 4 12 4.224 12 4.5 C 12 4.776 11.776 5 11.5 5 C 11.224 5 11 4.776 11 4.5 C 11 4.224 11.224 4 11.5 4 z M 8 5 C 9.657 5 11 6.343 11 8 C 11 9.657 9.657 11 8 11 C 6.343 11 5 9.657 5 8 C 5 6.343 6.343 5 8 5 z M 8 6 A 2 2 0 0 0 8 10 A 2 2 0 0 0 8 6 z"></path>
          </svg>
            </a>
        `;
            instafeed.appendChild(postElement);
        });
    }

    async function main() {
        const shortLivedToken = accessToken;
        const longLivedToken = await exchangeForLongLivedToken(shortLivedToken);
        console.log('Long-lived token:', longLivedToken);
        await displayInstagramPosts();
    }

    window.onload = main;
</script>

